{
  "name": "my-vite-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run watch-css\"",
    "build": "npm run clean && npm run build-css && tsc -b && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "rimraf dist && rimraf node_modules/.vite",
    "build-css": "sass --load-path=node_modules src/scss/styles.scss src/css/styles.css",
    "watch-css": "sass --load-path=node_modules --watch src/scss/styles.scss:src/css/styles.css"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.7",
    "@react-native-picker/picker": "^2.11.0",
    "axios": "^1.8.4",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "bootswatch": "^5.3.3",
    "date-fns": "^4.1.0",
    "devicon": "^2.16.0",
    "dotenv": "^16.4.7",
    "marked": "^15.0.7",
    "prismjs": "^1.30.0",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-bootstrap-icons": "^1.11.5",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-native-web": "^0.19.13",
    "react-router-dom": "^7.4.0",
    "signalr": "^2.4.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/axios": "^0.14.4",
    "@types/leaflet": "^1.9.17",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/react-native-web": "^0.19.0",
    "@types/xml2js": "^0.4.14",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.23.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "fontawesome-free": "^1.0.4",
    "globals": "^16.0.0",
    "leaflet": "^1.9.4",
    "react-leaflet": "^5.0.0",
    "rimraf": "^6.0.1",
    "sass": "^1.86.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.3",
    "vite-plugin-static-copy": "^2.3.0"
  }
}
