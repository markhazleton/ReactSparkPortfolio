{
  "name": "my-vite-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run watch-css\"",
    "build": "npm run clean && npm run build-css && tsc -b && npm run generate-seo-files && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "rimraf docs && rimraf node_modules/.vite",
    "build-css": "sass --load-path=node_modules src/scss/styles.scss src/css/styles.css",
    "watch-css": "sass --load-path=node_modules --watch src/scss/styles.scss:src/css/styles.css",
    "generate-sitemap": "node --loader ts-node/esm ./src/utils/generateSitemap.ts",
    "generate-robots": "node --loader ts-node/esm ./src/utils/generateRobotsTxt.ts",
    "generate-seo-files": "npm run generate-sitemap && npm run generate-robots"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.7",
    "@react-native-picker/picker": "^2.11.1",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "bootswatch": "^5.3.7",
    "date-fns": "^4.1.0",
    "devicon": "^2.16.0",
    "dotenv": "^17.2.0",
    "marked": "^16.1.1",
    "nvm": "^0.0.4",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-native-web": "^0.20.0",
    "react-router-dom": "^7.7.0",
    "signalr": "^2.4.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@rollup/plugin-strip": "^3.0.4",
    "@types/axios": "^0.14.4",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-native-web": "^0.19.1",
    "@types/xml2js": "^0.4.14",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fontawesome-free": "^1.0.4",
    "globals": "^16.3.0",
    "leaflet": "^1.9.4",
    "react-leaflet": "^5.0.0",
    "rimraf": "^6.0.1",
    "sass": "^1.89.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^7.0.5",
    "vite-plugin-static-copy": "^3.1.1"
  }
}
