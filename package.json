{
  "name": "my-vite-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && npm run build-css && tsc -b && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "rimraf dist && rimraf node_modules/.vite",
    "build-css": "sass --load-path=node_modules src/scss/styles.scss src/css/styles.css",
    "watch-css": "sass --watch src/scss/styles.scss:src/css/styles.css"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "bootswatch": "^5.3.3",
    "date-fns": "^4.1.0",
    "devicon": "^2.16.0",
    "dotenv": "^16.4.5",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.5",
    "react-bootstrap-icons": "^1.11.4",
    "react-dom": "^18.3.1",
    "react-native-web": "^0.19.12",
    "react-router-dom": "^6.27.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/axios": "^0.14.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-native-web": "^0.19.0",
    "@types/xml2js": "^0.4.14",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.12.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "fontawesome-free": "^1.0.4",
    "globals": "^15.11.0",
    "rimraf": "^6.0.1",
    "sass": "^1.79.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0",
    "vite": "^5.4.9",
    "vite-plugin-static-copy": "^2.0.0"
  }
}
