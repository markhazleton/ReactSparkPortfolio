{
  "name": "my-vite-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run watch-css\"",
    "build": "npm run clean && npm run build-css && tsc -b && npm run generate-seo-files && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "rimraf docs && rimraf node_modules/.vite",
    "build-css": "sass --load-path=node_modules src/scss/styles.scss src/css/styles.css",
    "watch-css": "sass --load-path=node_modules --watch src/scss/styles.scss:src/css/styles.css",
    "generate-sitemap": "node --loader ts-node/esm ./src/utils/generateSitemap.ts",
    "generate-robots": "node --loader ts-node/esm ./src/utils/generateRobotsTxt.ts",
    "generate-seo-files": "npm run generate-sitemap && npm run generate-robots"
  },
  "dependencies": {
    "@microsoft/signalr": "^9.0.6",
    "axios": "^1.12.2",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "date-fns": "^4.1.0",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@rollup/plugin-strip": "^3.0.4",
    "@types/axios": "^0.14.4",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fontawesome-free": "^1.0.4",
    "globals": "^16.4.0",
    "leaflet": "^1.9.4",
    "react-leaflet": "^5.0.0",
    "rimraf": "^6.0.1",
    "sass": "^1.92.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vite-plugin-static-copy": "^3.1.2"
  }
}
